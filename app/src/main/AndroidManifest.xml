<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.our.tripteller">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.mypage.ApplyFilterActivity"></activity>
        <activity android:name=".ui.mypage.ApplyRegionActivity" />
        <activity android:name=".ui.sign.ProfileActivity" />
        <activity android:name=".ui.sign.SigninActivity" />
        <activity android:name=".ui.sign.SignupActivity" />
        <activity android:name=".ui.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TermsActivity" />
        <activity android:name=".ui.chat.ChatRoom.ChatRoomActivity" />
        <activity android:name=".ui.home.Filter.FilterActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".ui.home.Detail.DetailActivity"
            android:theme="@style/noStatus" />
    </application>

</manifest>