<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.our.tripteller">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_tripteller_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_tripteller_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.mypage.SelectImageActivity"></activity>
        <activity
            android:name=".ui.sign.DatePickerActivity"
            android:theme="@android:style/Theme.Holo.Light.Dialog.NoActionBar" />
        <activity android:name=".ui.mypage.ApplyFilterActivity" />
        <activity android:name=".ui.home.detail.RequestActivity" />
        <activity android:name=".ui.mypage.ApplyImageActivity" />
        <activity android:name=".ui.mypage.ApplyIntroductionActivity" />
        <activity android:name=".ui.mypage.ApplyRegionActivity" />
        <activity android:name=".ui.sign.ProfileActivity" />
        <activity android:name=".ui.sign.SignupActivity" />
        <activity
            android:name=".ui.SplashActivity"
            android:theme="@style/transparentStatus">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TermsActivity" />
        <activity android:name=".ui.chat.chatroom.ChatRoomActivity" />
        <activity android:name=".ui.home.filter.FilterActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".ui.home.detail.DetailActivity"
            android:theme="@style/noStatus" />
    </application>

</manifest>